{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/components/banner/BannerRightComponent.jsx",
    "app/components/WithIntersectionObserver.js",
    "app/components/banner/SharpComponent.jsx",
    "app/admin/components/AdminAuth.tsx",
    "services/fetch.ts",
    "components/product-detail/product-detail-information/ProductDetail.Price.jsx"
  ],
  "exclude": ["node_modules"]
}
